{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": [
      "/static/*",
      "/assets/*",
      "/*.{png,jpg,gif,svg,js,css,woff,woff2}"
    ]
  },
  "mimeTypes": {
    ".json": "application/json",
    ".js": "application/javascript",
    ".css": "text/css",
    ".svg": "image/svg+xml",
    ".woff": "application/font-woff",
    ".woff2": "application/font-woff2"
  }
}
